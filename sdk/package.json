{
  "name": "@flowmint/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for FlowMint - Solana execution layer over Jupiter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.91.0"
  },
  "keywords": [
    "solana",
    "jupiter",
    "defi",
    "swap",
    "dca",
    "stop-loss",
    "payments"
  ],
  "author": "FlowMint Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flowmint/flowmint"
  },
  "bugs": {
    "url": "https://github.com/flowmint/flowmint/issues"
  },
  "homepage": "https://flowmint.io"
}
